<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="chat">
	
	<select id="selectMemberByNo" parameterType="int" resultType="memberVo">
		select * from member
		where mem_no = #{int}
	</select>	

</mapper>